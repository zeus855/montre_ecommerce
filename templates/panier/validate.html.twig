{% extends 'base.html.twig' %}

{% block title %}Panier!
{% endblock %}

{% block body %}


	<div class="container mt-5">
		{{ form_errors(form) }}

		{{ form_start(form) }}
		{{ form_row(form.label) }}
		{{ form_row(form.telephone) }}
		{{ form_row(form.rue) }}
		{{ form_row(form.codePostal) }}
		{{ form_row(form.ville)}}
		{{ form_row(form.pays) }}

		<button type="submit" class="btn btn-primary">Valider</button>

		{{ form_end(form) }}


	</div>


{% endblock %}
